x=15
y=6 #判断变量x和y是否相等
if(x==y):
print ("x等于y")
else:
print ("x 不等于y")
#判断变量x和y是否不相等
if(x!=y) :
print ("x 不等于y")
else:
print ("x等于y")
#判断变量x是否小于y
if( x<y) :
print ("x 小于y")
else:
print ("x 大于等于y")
#判断变量x是否大于y
if( x>y) :
print("x大于y")
else :
print (" x小于等于y")
#修改变量x和y的值
x=6
y=18
#判断变量x是否小于等于y
if(x<=y):
print ("x小于等于y")
else:
print ("x大于y")
#判断变量
x是否大于等于
if( y>=x):
print ("y大于等于x")
else:
print ("y小于x")


